{
  "name": "bubblesort",
  "version": "1.0.0",
  "description": "bubblesort performance benchmarking test",
  "author": "FireInjun",
  "repository": {
    "url": "https://github.com/fireinjun/bubblesort.git"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.js\"",
    "test": "jest",
    "pretty": "prettier --write --tab-width 4 \"src/**/*.js\"",
    "start": "node index.js"
  },
  "directories": {
    "src": "/src",
    "solutions": "/src/solutions",
    "testdata": "/src/testdata",
    "tests": "/src/tests",
    "results": "/results"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": false
  },
  "eslint": {
    "extends": [
      "airbnb", "prettier"
    ],
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": ["error"]
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": ["npm run pretty", "git add"]
  },
  "devDependencies": {
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  },
  "dependencies": {
    "benchmark": "^2.1.4",
    "microtime": "^2.1.8",
    "thread-sleep": "^2.1.0"
  }
}
